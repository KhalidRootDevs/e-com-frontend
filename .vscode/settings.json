{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.jsxSingleQuote": false,
  "prettier.printWidth": 140,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.endOfLine": "lf",
  "prettier.proseWrap": "never",
  "prettier.arrowParens": "avoid",
  "prettier.bracketSpacing": true,
  "prettier.semi": true,
  "prettier.trailingComma": "none",
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#0b90e9",
    "activityBar.background": "#0b90e9",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#fcc2e5",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#0b90e9",
    "statusBar.background": "#0972b8",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0b90e9",
    "statusBarItem.remoteBackground": "#0972b8",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#0972b8",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#0972b899",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#0972B8"
}
